{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.13.2",
    "dompurify": "^3.0.3",
    "marked": "^14.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "workbox-core": "^6.6.0",
    "workbox-expiration": "^6.6.0",
    "workbox-precaching": "^6.6.0",
    "workbox-routing": "^6.6.0",
    "workbox-strategies": "^6.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run postbuild",
    "postbuild": "node -e \"const fs = require('fs'); const path = require('path'); const src = path.join('public', 'service-worker.js'); const dest = path.join('build', 'service-worker.js'); if (fs.existsSync(src)) { fs.copyFileSync(src, dest); console.log('✅ service-worker.js copied to build'); } else { console.warn('⚠️ service-worker.js not found in public'); }\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve-build": "npx http-server ./build -p 8000 -a 0.0.0.0",
    "tunnel": "npx localtunnel --port 8000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "autoprefixer": "^10.4.14",
    "html-webpack-plugin": "^5.6.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.4.18"
  }
}
